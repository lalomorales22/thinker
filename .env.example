# Tinker API Configuration
TINKER_API_KEY=your_api_key_here

# Ollama Configuration (for AI Training Assistant)
# Install Ollama from https://ollama.ai and pull a model: ollama pull llama3.2
OLLAMA_URL=http://localhost:11434
OLLAMA_MODEL=llama3.2

# Backend Configuration
BACKEND_HOST=0.0.0.0
BACKEND_PORT=8000

# Optional: WandB for experiment tracking
WANDB_API_KEY=
WANDB_PROJECT=thinker

# Optional: Google Cloud for vector search (if using tool use features)
GOOGLE_GENAI_USE_VERTEXAI=
GCP_VERTEXAI_PROJECT_NUMBER=
GCP_VERTEXAI_REGION=
